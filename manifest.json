{
  "name": "Lounge prices",
  "manifest_version": 2,
  "version": "0.2",
  "permissions": [
    "https://*/",
	"storage",
	"tabs"
    ],
  "description": "Show item prices on csgolounge and dota2lounge sites.",
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "icons": {"48": "icon48.png",
          "128": "icon128.png" },
  "content_scripts": [
    {
      "matches": ["*://csgolounge.com/*"],
      "js": ["jquery-2.1.3.min.js", "csgolounge.js"],
      "run_at": "document_end"
    },
    {
      "matches": ["*://dota2lounge.com/*"],
      "js": ["jquery-2.1.3.min.js", "dota2lounge.js"],
      "run_at": "document_end"
    }
  ]
}
