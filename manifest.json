{
  "name": "CSGO Lounge prices",
  "manifest_version": 2,
  "version": "0.1",
  "permissions": [
    "https://*/",
	"storage",
	"tabs"
    ],
  "description": "Show prices on csgolounge items",
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "icons": {"48": "icon48.png",
          "128": "icon128.png" },
  "content_scripts": [
    {
      "matches": ["*://csgolounge.com/*"],
      "js": ["jquery-2.1.3.min.js", "myscript.js"],
      "run_at": "document_end"
    }
  ]
}